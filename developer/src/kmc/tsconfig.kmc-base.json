{
  "extends": "../../../tsconfig-base.json",

  "compilerOptions": {
    "module": "ES2022",
    "target": "es2022",
    "moduleResolution": "Node",
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "alwaysStrict": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "noUnusedLocals": true
  },
}
