<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE keyboard3 SYSTEM "../../../../../../../resources/standards-data/ldml-keyboards/techpreview/dtd/ldmlKeyboard3.dtd">
<keyboard3 locale="mt" conformsTo="techpreview">
  <names>
    <name value="keys-markers" />
  </names>

  <keys>
    <key id="ww" output="\m{w}" flicks="flickw" longPressKeyIds@@="\m{w}" longPressDefault="\m{w}" multiTap="\m{w}" />
    <flicks id="flickw">
        <flickSegment directions="nw" keyId@@="\m{w}" />
    </flicks>
  </keys>

  <layers formId="iso">
    <layer id="base">
      <row keys="ww" />
    </layer>
  </layers>

  <transforms type="simple">
    <transformGroup>
      <transform from="\m{w}" to="W" /> <!-- to quell marker err -->
    </transformGroup>
  </transforms>
</keyboard3>

<!-- you can have any output you want, as long as it's an W ! -->
