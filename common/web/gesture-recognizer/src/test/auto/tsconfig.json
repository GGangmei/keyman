 * test specs found under the `headless` subfolder.  Otherwise, it'll report errors while
 * editing - even if the tests themselves actually work.
 */
{
  "extends": "../../../../tsconfig.kmw-main-base.json",
  "compilerOptions": {
    "moduleResolution": "Node16",
  }
}
