{
  "extends": "../../../tsconfig-base.json",
  "compilerOptions": {
    "allowJs": false,
    "declaration": true,
    "module": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "inlineSources": true,
    "strict": true,
    "lib": ["es6"],
    "types": ["node"],
    "target": "es5",
    "baseUrl": "./",
    "outDir": "build/obj",
    "tsBuildInfoFile": "build/obj/tsconfig.tsbuildinfo",
    "rootDir": "./src"
  },
  "references": [
    { "path": "../types" },
    { "path": "../../web/utils" },
    { "path": "../../models/wordbreakers" }
  ],
  "include": [
	"src/**/*.ts"
  ],
  "exclude": [
	"test"
  ]
}
